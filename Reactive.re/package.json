    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bordoley/Reactive.re.git"
  },
  "devDependencies": {
    "bs-platform": "^3.1.3",
    "nyc": "^11.8.0",
    "reunit": "../reUnit"
  }
